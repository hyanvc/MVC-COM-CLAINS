<script src="https://code.jquery.com/jquery-1.9.1.js"></script>
<div class="form-group">
    <label for="exampleInputEmail1">Email address</label>
    <input type="email" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp" placeholder="Enter email">
    <small id="emailHelp" class="form-text text-muted">We'll never share your email with anyone else.</small>
</div>
<div class="form-group">
    <label for="exampleInputPassword1">Password</label>
    <input type="password" class="form-control" id="exampleInputPassword1" placeholder="Password">
</div>
<div class="form-group form-check">
    <input type="checkbox" class="form-check-input" id="exampleCheck1">
    <label class="form-check-label" id="permanente" for="exampleCheck1">Check me out</label>
</div>
<button type="submit" id="btenvi" class="btn btn-primary">Submit</button>

<script>


    $(document).ready(function () {
        $("#btenvi").click(function () {
            var senha = $("#exampleInputPassword1").val();
            var email = $("#exampleInputEmail1").val();
            var permanente = $("input[type='checkbox']").val();

            var bb = "Hyan";

            $.ajax({
                url: "https://localhost:7237/Login/Login",
                data: { "PassWord": senha, "EMAIL": email, "KeepLoggedIn": permanente },
                type: 'POST'
            })
                .done(function (data) {
                    //window.location.href = "Index/?NM_NOME=" + bb;
                    window.location.href = "https://localhost:7237";

                });
        });


    });

</script>